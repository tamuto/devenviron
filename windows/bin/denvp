# TODO ~/.devenviron is not exist then makedir 
podman run \
    --rm -it \
    -v $PWD:$PWD -w $PWD \
    -v ~/.devenviron/.gitconfig:/root/.gitconfig \
    -v ~/.devenviron/.git-credentials:/root/.git-credentials \
    -v ~/.devenviron/.awsconfig:/root/.awsconfig \
    -v ~/.devenviron/.aws-credentials:/root/.aws-credentials \
    -p $1 \
    tamuto/devenviron:latest ${@:2}
